class Calculator {
  public static void main(String[] args) {
    int a = Integer.parseInt(args[0]);
    int b = Integer.parseInt(args[1]);
    System.out.println("Add=" + (a + b));
    System.out.println("Sub=" + (a - b));
    System.out.println("Mul=" + (a * b));
    System.out.println("Div=" + (a / b));
  }
}


import java.util.Scanner;

class Age {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    int birth = sc.nextInt();
    System.out.println("You are " + (2024 - birth) + " years old.");
  }
}


import java.util.Scanner;

class BMI {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    double w = sc.nextDouble();
    double h = sc.nextDouble();
    System.out.printf("Your BMI is %.2f", w / (h * h));
  }
}


import java.util.Scanner;

class OddEven {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    int n = sc.nextInt();
    if (n % 2 == 0) {
      System.out.println("Even");
    } else {
      System.out.println("Odd");
    }
  }
}


import java.util.*;

class ITCity {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    String city = sc.next();
    String[] cities = {"Delhi", "Mumbai", "Kolkatta", "Bangalore", "Chennai", "Hyderabad"};
    boolean found = false;
    for (int i = 0; i < cities.length; i++) {
      if (city.equals(cities[i])) {
        found = true;
        break;
      }
    }
    if (found) {
      System.out.println("IT City");
    } else {
      System.out.println("Not IT City");
    }
  }
}

import java.util.*;
class Grade {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int s = sc.nextInt();
        if (s >= 90)
            System.out.println("Your grade is A.");
        else if (s >= 80)
            System.out.println("Your grade is B.");
        else if (s >= 70)
            System.out.println("Your grade is C.");
        else if (s >= 60)
            System.out.println("Your grade is D.");
        else
            System.out.println("Your grade is F.");
    }
}

import java.util.*;
class Largest {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt(), b = sc.nextInt(), c = sc.nextInt();
        if (a > b && a > c)
            System.out.println("The largest number is " + a);
        else if (b > c)
            System.out.println("The largest number is " + b);
        else
            System.out.println("The largest number is " + c);
    }
}

import java.util.*;
class Triangle {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt(), b = sc.nextInt(), c = sc.nextInt();
        if (a == b && b == c)
            System.out.println("The triangle is equilateral.");
        else if (a == b || b == c || a == c)
            System.out.println("The triangle is isosceles.");
        else
            System.out.println("The triangle is scalene.");
    }
}


import java.util.*;
class Password {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String p = sc.next();
        boolean up = false, low = false, dig = false, sp = false;
        for (int i = 0; i < p.length(); i++) {
            char ch = p.charAt(i);
            if (Character.isUpperCase(ch)) up = true;
            else if (Character.isLowerCase(ch)) low = true;
            else if (Character.isDigit(ch)) dig = true;
            else sp = true;
        }
        if (p.length() >= 8 && up && low && dig && sp)
            System.out.println("Password strength: Strong.");
        else if (p.length() >= 6 && up && low && dig)
            System.out.println("Password strength: Medium.");
        else
            System.out.println("Password strength: Weak.");
    }
}
import java.util.*;
class Reverse {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt(), rev = 0;
        while (n > 0) {
            rev = rev * 10 + n % 10;
            n /= 10;
        }
        System.out.println("The reversed number is " + rev);
    }
}



import java.util.*;
class SumDigits {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int sum = 0;
        while (n > 0) {
            sum += n % 10;
            n /= 10;
        }
        System.out.println("The sum of the digits is " + sum);
    }
}



class Pattern {
    public static void main(String[] args) {
        for (int i = 1; i <= 9; i += 2) {
            for (int j = 1; j <= i; j++)
                System.out.print("* ");
            System.out.println();
        }
        for (int i = 7; i >= 1; i -= 2) {
            for (int j = 1; j <= i; j++)
                System.out.print("* ");
            System.out.println();
        }
    }
}







